<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>LorinthsLair.UnitConverter</PackageId>
    <Authors>Jerzy Kruszewski</Authors>
    <Company>LorinthsLair</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/JerzyKruszewski/UnitConverter</RepositoryUrl>
    <PackageTags>Units, Converter, UnitConverter, LorinthsLair</PackageTags>
    <PackageIcon>LorinthsLair.png</PackageIcon>
    <Description>Simple .NET Standard library for unit conversions.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Changed namespace form UnitConverter to LorinthsLair.UnitConverter
Added time and speed converters</PackageReleaseNotes>
    <RootNamespace>LorinthsLair.UnitConverter</RootNamespace>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LorinthsLair.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
